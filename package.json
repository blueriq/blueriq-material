{
  "name": "blueriq-material",
  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://github.com/blueriq/blueriq-material",
  "keywords": [
    "blueriq",
    "theme"
  ],
  "scripts": {
    "ng": "ng",
    "ng:version": "ng --version",
    "browserslist": "npx browserslist",
    "start": "ng serve --open --host 0.0.0.0 --proxy-config proxy.conf.js",
    "start:ie": "yarn start --configuration es5",
    "start:nolive": "yarn start --live-reload false --watch false",
    "build": "ng build --prod",
    "build:runtime": "ng build --prod -c runtime",
    "test": "ng test",
    "lint": "yarn tslint && yarn sass-lint",
    "tslint": "ng lint",
    "sass-lint": "sass-lint --verbose --config sass-lint.yml src/**/*.scss",
    "verify": "ng test --watch false --code-coverage true",
    "verify:all": "yarn verify && yarn tslint && yarn sass-lint",
    "e2e": "yarn --ignore-engines protractor e2e/protractor.conf.js ",
    "e2e:dev": "yarn webdriver-manager update && yarn protractor e2e/protractor-dev.conf.js",
    "docs": "compodoc --config compodoc.json",
    "version:increment": "npm version --allow-same-version"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "IE 9-11"
  ],
  "engines": {
    "yarn": "^1.4.0"
  },
  "dependencies": {
    "@angular/animations": "9.1.9",
    "@angular/cdk": "9.2.4",
    "@angular/common": "9.1.9",
    "@angular/compiler": "9.1.9",
    "@angular/core": "9.1.9",
    "@angular/flex-layout": "9.0.0-beta.31",
    "@angular/forms": "9.1.9",
    "@angular/material": "9.2.4",
    "@angular/material-moment-adapter": "9.2.4",
    "@angular/platform-browser": "9.1.9",
    "@angular/platform-browser-dynamic": "9.1.9",
    "@angular/router": "9.1.9",
    "@blueriq/angular": "0.8.0",
    "@blueriq/core": "0.8.0",
    "@ngrx/effects": "9.2.0",
    "@ngrx/store": "9.2.0",
    "@ngrx/store-devtools": "9.2.0",
    "chart.js": "2.9.3",
    "core-js": "3.6.5",
    "eventsource": "1.0.7",
    "font-awesome": "4.7.0",
    "hammerjs": "2.0.8",
    "material-icons": "0.3.1",
    "moment": "2.24.0",
    "ng-pick-datetime": "7.0.0",
    "ng-pick-datetime-moment": "1.0.7",
    "ng2-file-upload": "1.4.0",
    "roboto-fontface": "0.10.0",
    "rxjs": "6.5.5",
    "tslib": "1.13.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.7",
    "@angular/cli": "9.1.7",
    "@angular/compiler-cli": "9.1.9",
    "@angular/language-service": "9.1.9",
    "@compodoc/compodoc": "1.1.11",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "5.2.2",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.0.9",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "3.3.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "karma-junit-reporter": "2.0.1",
    "protractor": "5.4.3",
    "protractor-jasmine2-html-reporter": "0.0.7",
    "sass-lint": "1.13.1",
    "ts-node": "8.10.2",
    "tslint": "6.1.2",
    "typescript": "3.8.3",
    "webdriver-manager": "12.1.7"
  },
  "resolutions": {
    "eslint": "^4.18.2",
    "marked": "^0.7.0",
    "minimist": "^1.2.3"
  }
}
