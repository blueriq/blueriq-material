{
  "name": "blueriq-material",
  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://github.com/blueriq/blueriq-material",
  "keywords": [
    "blueriq",
    "theme"
  ],
  "scripts": {
    "ng": "ng",
    "ng:version": "ng --version",
    "browserslist": "npx browserslist",
    "start": "ng serve --open --host 0.0.0.0 --proxy-config proxy.conf.js",
    "start:ie": "yarn start --configuration es5",
    "start:nolive": "yarn start --live-reload false --watch false",
    "build": "ng build --prod",
    "build:runtime": "ng build --prod -c runtime",
    "test": "ng test",
    "lint": "yarn tslint && yarn sass-lint",
    "tslint": "ng lint",
    "sass-lint": "sass-lint --verbose --config sass-lint.yml src/**/*.scss",
    "verify": "ng test --watch false --code-coverage true",
    "verify:all": "yarn verify && yarn tslint && yarn sass-lint",
    "e2e": "yarn --ignore-engines protractor e2e/protractor.conf.js ",
    "e2e:dev": "yarn webdriver-manager update && yarn protractor e2e/protractor-dev.conf.js",
    "docs": "compodoc --config compodoc.json",
    "version:increment": "npm version --allow-same-version"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "IE 9-11"
  ],
  "engines": {
    "yarn": "^1.4.0"
  },
  "dependencies": {
    "@angular/animations": "8.2.4",
    "@angular/cdk": "8.1.4",
    "@angular/common": "8.2.4",
    "@angular/compiler": "8.2.4",
    "@angular/core": "8.2.4",
    "@angular/flex-layout": "8.0.0-beta.27",
    "@angular/forms": "8.2.4",
    "@angular/material": "8.1.4",
    "@angular/material-moment-adapter": "8.1.4",
    "@angular/platform-browser": "8.2.4",
    "@angular/platform-browser-dynamic": "8.2.4",
    "@angular/router": "8.2.4",
    "@blueriq/angular": "0.7.7",
    "@blueriq/core": "0.7.7",
    "@ngrx/effects": "7.2.0",
    "@ngrx/store": "7.2.0",
    "@ngrx/store-devtools": "7.2.0",
    "chart.js": "2.7.3",
    "core-js": "3.2.1",
    "eventsource": "1.0.7",
    "font-awesome": "4.7.0",
    "hammerjs": "2.0.8",
    "material-icons": "0.3.1",
    "moment": "2.24.0",
    "ng-pick-datetime": "7.0.0",
    "ng-pick-datetime-moment": "1.0.7",
    "ng2-file-upload": "1.3.0",
    "roboto-fontface": "0.10.0",
    "rxjs": "6.5.2",
    "web-animations-js": "2.3.2",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.4",
    "@angular/cli": "8.3.2",
    "@angular/compiler-cli": "8.2.4",
    "@angular/language-service": "8.2.4",
    "@compodoc/compodoc": "1.1.9",
    "@types/jasmine": "2.8.9",
    "@types/jasminewd2": "2.0.5",
    "@types/node": "10.11.6",
    "codelyzer": "5.0.1",
    "jasmine-core": "3.3.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "karma-junit-reporter": "1.2.0",
    "merge": "1.2.1",
    "protractor": "5.4.2",
    "protractor-jasmine2-html-reporter": "0.0.7",
    "sass-lint": "1.13.1",
    "ts-node": "8.0.2",
    "tslint": "5.19.0",
    "typescript": "3.5.3",
    "webdriver-manager": "12.1.6"
  },
  "resolutions": {
    "eslint": "^4.18.2",
    "fstream": "^1.0.12",
    "handlebars": "^4.1.2",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.13",
    "lodash.mergewith": "^4.6.2",
    "marked": "^0.6.2",
    "serialize-javascript": "^2.1.2",
    "tar": "^4.4.2"
  }
}
