{
  "name": "blueriq-material",
  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://github.com/blueriq/blueriq-material",
  "keywords": [
    "blueriq",
    "theme"
  ],
  "scripts": {
    "ng": "ng",
    "ng:version": "ng --version",
    "browserslist": "npx browserslist",
    "start": "ng serve --open --host 0.0.0.0 --proxy-config proxy.conf.js",
    "start:ie": "yarn start --configuration es5",
    "start:nolive": "yarn start --live-reload false --watch false",
    "build": "ng build --prod",
    "build:runtime": "ng build --prod -c runtime",
    "test": "ng test",
    "lint": "yarn tslint && yarn sass-lint",
    "tslint": "ng lint",
    "sass-lint": "sass-lint --verbose --config sass-lint.yml src/**/*.scss",
    "verify": "ng test --watch false --code-coverage true",
    "verify:all": "yarn verify && yarn tslint && yarn sass-lint",
    "e2e": "yarn --ignore-engines protractor e2e/protractor.conf.js ",
    "e2e:dev": "yarn webdriver-manager update && yarn protractor e2e/protractor-dev.conf.js",
    "docs": "compodoc --config compodoc.json",
    "version:increment": "npm version --allow-same-version"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "IE 11"
  ],
  "engines": {
    "yarn": "^1.22.0"
  },
  "dependencies": {
    "@angular/animations": "11.2.12",
    "@angular/cdk": "11.2.11",
    "@angular/common": "11.2.12",
    "@angular/compiler": "11.2.12",
    "@angular/core": "11.2.12",
    "@angular/flex-layout": "11.0.0-beta.33",
    "@angular/forms": "11.2.12",
    "@angular/material": "11.2.11",
    "@angular/material-moment-adapter": "11.2.11",
    "@angular/platform-browser": "11.2.12",
    "@angular/platform-browser-dynamic": "11.2.12",
    "@angular/router": "11.2.12",
    "@blueriq/angular": "0.10.1",
    "@blueriq/core": "0.10.1",
    "@danielmoncada/angular-datetime-picker": "11.0.1",
    "@ngrx/effects": "11.1.1",
    "@ngrx/store": "11.1.1",
    "@ngrx/store-devtools": "11.1.1",
    "chart.js": "2.9.4",
    "core-js": "3.8.3",
    "eventsource": "1.1.0",
    "font-awesome": "4.7.0",
    "material-icons": "0.3.1",
    "moment": "2.24.0",
    "ng2-file-upload": "1.4.0",
    "roboto-fontface": "0.10.0",
    "rxjs": "6.6.6",
    "tslib": "2.2.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.11",
    "@angular/cli": "11.2.11",
    "@angular/compiler-cli": "11.2.12",
    "@angular/language-service": "11.2.12",
    "@compodoc/compodoc": "1.1.11",
    "@types/jasmine": "3.6.6",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "6.0.2",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.2",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "karma-junit-reporter": "2.0.1",
    "protractor": "7.0.0",
    "protractor-jasmine2-html-reporter": "0.0.7",
    "sass-lint": "1.13.1",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.1.5",
    "webdriver-manager": "12.1.8"
  },
  "resolutions": {
    "eslint": "^4.18.2"
  }
}
